<br />
<div class="booyah-box col-xs-6 col-xs-offset-3">
  <h1>Add a new course</h1>
  <%= simple_form_for @course, url: instructor_courses_path do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :cost %>
    <br />
    <%= f.submit "Create Course", class: 'btn btn-primary' %>
    <br />
  <% end %>
</div>
